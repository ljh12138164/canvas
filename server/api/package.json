{
  "name": "api",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/api.js",
  "exports": {
    ".": {
      "types": "./dist/hono/index.d.ts",
      "import": "./dist/hono/index.js",
      "require": "./dist/hono/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "pnpm run api",
    "restart": "rimraf dist && vite build && tsc",
    "api": "rimraf dist && vite build && tsc && vercel dev --listen=8000",
    "local": "bun run --hot ./local.ts",
    "deploy:prod": "vercel --prod",
    "deploy": "vercel",
    "zhipu": "bun run --hot --port=1234 ./zhipu.ts"
  },
  "dependencies": {
    "@clerk/backend": "^1.17.2",
    "@google/generative-ai": "^0.21.0",
    "@hono/clerk-auth": "^2.0.0",
    "@hono/node-server": "1.13.7",
    "@hono/zod-validator": "^0.4.1",
    "@react-email/components": "0.0.28",
    "@supabase/supabase-js": "^2.46.1",
    "@tanstack/vue-table": "^8.20.5",
    "@types/bcryptjs": "2.4.0",
    "@types/blueimp-md5": "^2.18.2",
    "@types/node": "^20",
    "@types/react": "^18.3.11",
    "@vitejs/plugin-react": "^4.3.3",
    "await-to-js": "^3.0.0",
    "bcryptjs": "^2.4.0",
    "blueimp-md5": "^2.19.0",
    "hono": "^4.6.13",
    "jose": "^5.9.6",
    "nanoid": "^5.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^4.0.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "unsplash-js": "^7.0.19",
    "zhipuai-sdk-nodejs-v4": "^0.1.12",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "drizzle-kit": "^0.28.1",
    "vercel": "^39.2.0",
    "vite": "^6.0.1"
  }
}
