    "bcryptjs": "^2.4.0",
    "blueimp-md5": "^2.19.0",
    "hono": "^4.6.0",
    "jose": "^5.9.6",
    "nanoid": "^5.0.8",
    "resend": "^4.0.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "unsplash-js": "^7.0.19",
    "zod": "^3.23.8",
