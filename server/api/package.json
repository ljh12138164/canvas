{
  "name": "api",
  "type": "module",
  "files": ["dist"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "npm-run-all --parallel api builds",
    "lint": " biome check --fix --unsafe --no-errors-on-unmatched   && biome check --write --no-errors-on-unmatched && biome check --write --organize-imports-enabled=false --no-errors-on-unmatched && biome check --write --unsafe --no-errors-on-unmatched && biome check --files-ignore-unknown=true&& biome lint --write --no-errors-on-unmatched",
    "builds": "tsup --watch",
    "tsup": "tsup",
    "api": "vercel dev --listen=8000",
    "local": "bun run --hot ./local.ts",
    "deploy:prod": "vercel --prod",
    "doc": "bun run --hot --port=6666 ./src/hono/doc/index.ts",
    "deploy": "vercel",
    "zhipu": "bun run --hot --port=1234 ./zhipu.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@hono/clerk-auth": "^2.0.0",
    "@hono/node-server": "1.13.8",
    "@hono/swagger-ui": "^0.5.0",
    "@hono/zod-openapi": "^0.18.3",
    "@hono/zod-validator": "^0.4.2",
    "@react-email/components": "0.0.32",
    "@supabase/postgrest-js": "^1.18.1",
    "@supabase/supabase-js": "^2.48.1",
    "@tanstack/vue-table": "^8.20.5",
    "@types/bcryptjs": "2.4.6",
    "@types/blueimp-md5": "^2.18.2",
    "@types/exceljs": "^1.3.2",
    "await-to-js": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "blueimp-md5": "^2.19.0",
    "exceljs": "^4.4.0",
    "hono": "^4.6.20",
    "jose": "^5.9.6",
    "nanoid": "^5.0.9",
    "npm-run-all": "^4.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "resend": "^4.1.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "unsplash-js": "^7.0.19",
    "zhipuai-sdk-nodejs-v4": "^0.1.12",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.7",
    "vercel": "^39.3.0"
  }
}
